//import java.io.*;
import java.util.Scanner;
class AddMultiply{
	public static void main(String args[]){
		int b=80,sum,multiply;
		Scanner num = new Scanner(System.in);
		System.out.println("enter a");
		int a = num.nextInt();
		sum = a+b;
		System.out.println("sum = "+sum);
		
		
		
		System.out.println("multiply");
		multiply = a*b;
		
		System.out.println(multiply);
		
		}
		}
